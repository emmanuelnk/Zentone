al":{"line":39,"column":4}},{"name":"io","generated":{"line":40,"column":16},"source":"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js","original":{"line":39,"column":16}},{"generated":{"line":40,"column":18},"source":"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js","original":{"line":39,"column":4}},{"name":"span","generated":{"line":40,"column":20},"source":"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js","original":{"line":39,"column":20}},{"generated":{"line":40,"column":24},"source":"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js","original":{"line":39,"column":4}},{"generated":{"line":41,"column":0},"source":"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js","original":{"line":40,"column":3}},{"generated":{"line":41,"column":3},"source":"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js","original":{"line":17,"column":2}},{"generated":{"line":42,"column":0},"source":"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js","original":{"line":41,"column":1}},{"generated":{"line":42,"column":1},"source":"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js","original":{"line":6,"column":0}}],"sources":{"node_modules/express-status-monitor/src/helpers/gather-os-metrics.js":"const pidusage = require('pidusage');\nconst os = require('os');\nconst sendMetrics = require('./send-metrics');\nconst debug = require('debug')('express-status-monitor');\n\nmodule.exports = (io, span) => {\n  const defaultResponse = {\n    2: 0,\n    3: 0,\n    4: 0,\n    5: 0,\n    count: 0,\n    mean: 0,\n    timestamp: Date.now(),\n  };\n\n  pidusage.stat(process.pid, (err, stat) => {\n    if (err) {\n      debug(err);\n      return;\n    }\n\n    const last = span.responses[span.responses.length - 1];\n\n    // Convert from B to MB\n    stat.memory = stat.memory / 1024 / 1024;\n    stat.load = os.loadavg();\n    stat.timestamp = Date.now();\n\n    span.os.push(stat);\n    if (!span.responses[0] || last.timestamp + (span.interval * 1000) < Date.now()) {\n      span.responses.push(defaultResponse);\n    }\n\n    // todo: I think this check should be moved somewhere else\n    if (span.os.length >= span.retention) span.os.shift();\n    if (span.responses[0] && span.responses.length > span.retention) span.responses.shift();\n\n    sendMetrics(io, span);\n  });\n};\n"},"lineCount":null}},"hash":"aeb9f3b56e8356f443953d4be95f5c14","cacheData":{"env":{}}}