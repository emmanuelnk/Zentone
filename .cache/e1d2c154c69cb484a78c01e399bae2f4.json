onfig);\n      }\n    });\n\n    config.spans.forEach(span => {\n      span.os = [];\n      span.responses = [];\n      const interval = setInterval(() => gatherOsMetrics(io, span), span.interval * 1000);\n\n      // Don't keep Node.js process up\n      interval.unref();\n    });\n  }\n};\n"},"lineCount":null}},"hash":"66b49566a5d712f63cfb35687e4282f5","cacheData":{"env":{}}}